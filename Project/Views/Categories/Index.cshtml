@using DomainModelEntity;
@model List<Category>

<div class="d-flex flex-wrap">
	@foreach (var category in Model)
	{
		<div class="card m-1" style="width: 18rem;">
			<div class="card-body">
				<h5 class="card-title">@category.CategoryName</h5>
				<p class="card-text">@category.Description</p>
			</div>
		</div>
	}
</div>